{
  "extends": "../tslint.json",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "indent": false,
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
